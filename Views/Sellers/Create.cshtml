@model CSharpSecaoDezoito.Models.Seller

@{
    ViewData["Title"] = "Create";
}

<h2><img src="https://img.icons8.com/color/48/000000/c-sharp-logo-2.png"/>@ViewData["Title"]</h2>

<h4>Seller</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label class="btn btn-info disabled" asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
            </div>
             <div class="form-group">
                <label class="btn btn-warning disabled" asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" />
            </div>
             <div class="form-group">
                <label class="btn btn-secondary disabled" asp-for="BirthDate" class="control-label"></label>
                <input asp-for="BirthDate" class="form-control" />
            </div>
             <div class="form-group">
                <label class="btn btn-danger disabled" asp-for="BaseSalary" class="control-label"></label>
                <input asp-for="BaseSalary" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-success" />
            </div>
            <div class="progress">
                 <div class="progress-bar progress-bar-striped progress-bar-animated bg-success" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%;"></div>
            </div>
        </form>
    </div>
</div>